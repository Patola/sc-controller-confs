{
    "_": "", 
    "buttons": {
        "A": {
            "action": "mode(LGRIP, button(Keys.KEY_B), button(Keys.KEY_F))"
        }, 
        "B": {
            "action": "mode(LGRIP, button(Keys.KEY_RIGHTBRACE), button(Keys.KEY_1))"
        }, 
        "BACK": {
            "action": "button(Keys.KEY_ESC)", 
            "name": "Escape"
        }, 
        "C": {
            "action": "hold(menu('Default.menu'), menu('Default.menu'))"
        }, 
        "LB": {
            "action": "mode(LGRIP, mouse(Rels.REL_WHEEL, 1), button(Keys.KEY_SPACE))"
        }, 
        "RB": {
            "action": "mode(LGRIP, mouse(Rels.REL_WHEEL, -1), button(Keys.KEY_LEFTCTRL))"
        }, 
        "RGRIP": {
            "action": "button(Keys.KEY_LEFTSHIFT)"
        }, 
        "RPAD": {
            "action": "button(Keys.BTN_MIDDLE)"
        }, 
        "START": {
            "action": "mode(LGRIP, button(Keys.KEY_TAB), button(Keys.KEY_ENTER))"
        }, 
        "STICKPRESS": {
            "action": "mode(LGRIP, button(Keys.KEY_TAB), button(Keys.KEY_LEFTALT))"
        }, 
        "X": {
            "action": "mode(LGRIP, button(Keys.KEY_LEFTBRACE), button(Keys.KEY_2))"
        }, 
        "Y": {
            "action": "mode(LGRIP, button(Keys.KEY_GRAVE), button(Keys.KEY_3))"
        }
    }, 
    "cpad": {}, 
    "dpad": {}, 
    "gyro": {}, 
    "is_template": false, 
    "menus": {}, 
    "pad_left": {
        "action": "mode(LGRIP, click(dpad(1, button(Keys.KEY_T), button(Keys.KEY_C), button(Keys.KEY_PAGEDOWN), button(Keys.KEY_PAGEUP))), click(dpad(1, button(Keys.KEY_I), button(Keys.KEY_M), button(Keys.KEY_END), button(Keys.KEY_HOME))))"
    }, 
    "pad_right": {
        "action": "mode(LGRIP, sens(1.0, -1.0, XY(mouse(Rels.REL_HWHEEL), mouse(Rels.REL_WHEEL))), smooth(8, 0.78, 2.0, mouse()))"
    }, 
