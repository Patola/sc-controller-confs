        "action": "click(dpad(button(Keys.KEY_F), button(Keys.KEY_C), button(Keys.KEY_Q), button(Keys.KEY_E)))"
    }, 
    "pad_right": {
        "action": "smooth(8, 0.78, 2.0, mouse())"
    }, 
