        }, 
        "STICKPRESS": {
            "action": "button(Keys.KEY_TAB)", 
            "name": "Build Mode"
        }, 
        "X": {
            "action": "button(Keys.KEY_R)", 
            "name": "Secondary Action"
        }, 
        "Y": {
            "action": "button(Keys.KEY_F)", 
            "name": "Flashlight"
        }
