        "action": "click(dpad(1, button(Keys.KEY_I), button(Keys.KEY_DOWN), button(Keys.KEY_T), button(Keys.KEY_J)))"
    }, 
    "pad_right": {
        "action": "mode(LGRIP, sens(1.0, -1.0, XY(mouse(Rels.REL_HWHEEL), mouse(Rels.REL_WHEEL))), smooth(8, 0.78, 2.0, mouse()))"
